<div class="wrapper-main">
  <h1>Edit Session</h1>
  <div class="row">
    <div class="col-md-4 text-center">
      <%= image_tag('student.png') %>
    </div>
    <div class="col-md-6 col-md-offset-1">
      <%= form_for [@student, @session] do |f| %>
      <div class="form-group">
        <%= f.label :session_date %>
        <%= f.text_field :session_date, class: 'form-control', placeholder: "StudentLastName_TutorLastName SessionEndDate" %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
        <%= link_to "Cancel", :back, {:class=>"btn btn-danger"} %>
      </div>
      <% end %>
    </div>
  </div>
  <%= params.inspect %>
</div>
