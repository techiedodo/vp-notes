<div class="wrapper-main">
  <ul>
    <li>
      Date of Session:
      <br><%= @note.date_session %>
    </li>
    <li>
      Summary Statement:
      <br><%= @note.summary %>
    </li>
    <li>
      What We've Achieved:
      <br><%= @note.achievement %>
    </li>
    <li>
      What Comes Next:
      <br><%= @note.what_next %>
    </li>
    <li>
      Recommendations:
      <br><%= @note.recommendation %>
    </li>
    <li>
      Rate:
      <br><%= @note.rate %>
    </li>
    <li>
      Tutoring Start Time:
      <br><%= @note.tm_start %>
    </li>
    <li>
      Tutoring Start Time:
      <br><%= @note.tm_end %>
    </li>
    <li>
      Elapsed Time:
      <br><%= (@note.tm_end - @note.tm_start)/3600 %>
      Hrs.
    </li>
    <li>
      Session Total:
      <br>$<%= ((@note.tm_end - @note.tm_start)/3600)*@note.rate %>
      Hrs.
    </li>
    <%= link_to "Notes List", student_tutoring_session_path(@note.tutoring_session.student_id, @note.tutoring_session) %>
    |
    <%= link_to "Edit", edit_note_path %>
    |
    <%= link_to "Delete", @note, method: :delete, data: {confirm: 'Are you sure that you want to delete this session?'}   %>
  </ul>
  <hr>
  <%= params.inspect %>
</div>
